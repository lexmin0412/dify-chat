    "@rsbuild/plugin-react": "^1.1.1",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/semver": "^7.5.8",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "tailwindcss": "3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  }
}
