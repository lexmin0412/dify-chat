{
  "name": "dify-chat-web",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build:pkgs": "pnpm --filter @dify-chat/* build",
    "build": "pnpm build:pkgs && rsbuild build",
    "dev": "pnpm build:pkgs && pnpm dev:main",
    "dev:main": "rsbuild dev",
    "format": "prettier --write .",
    "lint": "eslint .",
    "preview": "rsbuild preview"
  },
  "packageManager": "pnpm@8.9.2",
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/x": "^1.0.6",
    "@dify-chat/api": "workspace:^",
    "@dify-chat/components": "workspace:^",
    "@dify-chat/core": "workspace:^",
    "@dify-chat/helpers": "workspace:^",
    "@toolkit-fe/clipboard": "^0.1.22",
    "@toolkit-fe/where-am-i": "^0.1.22",
    "ahooks": "^3.8.4",
    "antd": "^5.24.4",
    "antd-style": "^3.7.1",
    "markdown-it": "^14.1.0",
    "openai": "^4.89.0",
    "pure-react-router": "^0.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semver": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.22.0",
    "@rsbuild/core": "^1.2.19",
